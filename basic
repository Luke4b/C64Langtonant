10 PRINT CHR$(147)
20 A = 1524
30 C = 0
35 D = 0

40 B = PEEK(A)
45 D = D + 1
46 IF D = 20 THEN STOP

50 IF B = 32 THEN GOTO 100
60 IF B = 160 THEN GOTO 150

100 C = C + 1
110 POKE A,160
120 GOTO 180

150 C = C - 1
160 POKE A,32

180 IF C = 4 THEN C = 0
190 IF C = -1 THEN C = 4
200 IF C = 0 THEN A = A - 40
210 IF C = 1 THEN A = A + 1
220 IF C = 2 THEN A = A + 40
230 IF C = 3 THEN A = A - 1
240 GOTO 40
